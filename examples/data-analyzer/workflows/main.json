{
  "name": "Analysis Flow",
  "version": "1.0.0",
  "graph": {
    "nodes": [
      {
        "id": "upload",
        "agent": "UploadParserAgent"
      },
      {
        "id": "insight",
        "agent": "InsightGeneratorAgent"
      }
    ],
    "edges": [
      {
        "from": "upload",
        "to": "insight"
      }
    ]
  },
  "io": {
    "inputs": [
